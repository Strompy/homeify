<h1>Sign Up for Homeify</h1>

<%= form_with model: @user do |f| %>
  <%= f.label :email, 'Email' %>
  <%= f.text_field :email %>
  <%= f.label :password, 'Password' %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation, 'Password Confirmation' %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit 'Sign Up' %>
<% end %>
